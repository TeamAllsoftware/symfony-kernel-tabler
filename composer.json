{
    "name": "allsoftware/symfony-kernel-tabler",
    "description": "Base 'kernel' bundle to be used for re-used functionality in symfony with Tabler.io",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian CAVASIN"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^8.0",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "2.*",
        "kevinpapst/tabler-bundle": "^0.6",
        "ramsey/uuid-doctrine": "^1.8",
        "symfony/asset": "5.*",
        "symfony/config": "5.*",
        "symfony/dependency-injection": "5.*",
        "symfony/event-dispatcher": "5.*",
        "symfony/form": "5.*",
        "symfony/http-foundation": "5.*",
        "symfony/http-kernel": "5.*",
        "symfony/intl": "5.*",
        "symfony/mailer": "5.*",
        "symfony/options-resolver": "5.*",
        "symfony/property-access": "5.*",
        "symfony/security-core": "5.*",
        "symfony/translation": "5.*",
        "symfony/twig-bridge": "5.*",
        "symfony/validator": "5.*",
        "twig/twig": "^3.0",
        "ext-gmp": "*"
    },
    "require-dev": {
        "phpspec/prophecy": "^1.6",
        "symfony/framework-bundle" : "5.*"
    },
    "autoload": {
        "psr-4": {
            "Allsoftware\\SymfonyKernelTabler\\": "src/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*"
        }
    }
}
